{
  "name": "site-me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "docker-compose:restart": "docker-compose down && docker-compose up -d && dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:local:flush": "rm -r prisma/migrations && dotenv -e .env.local -- npx prisma migrate dev"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@prisma/client": "^5.7.1",
    "@react-rxjs/core": "^0.10.3",
    "@react-rxjs/utils": "^0.9.5",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/styled-jsx": "^3.4.4",
    "next": "13.1.1",
    "oci-sdk": "^2.74.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.2",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "^3.16.3",
    "eslint": "8.23.1",
    "eslint-config-next": "^13.1.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^5.7.1",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "src/**/*.{html,ts,scss}": [
      "prettier --config .prettierrc.yaml --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
